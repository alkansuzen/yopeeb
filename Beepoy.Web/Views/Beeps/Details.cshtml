@model Beepoy.Web.Models.Beep

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Details</h2>

<fieldset>
    <legend>Beep</legend>

    <div class="display-label">Text</div>
    <div class="display-field">@Model.Text</div>

    <div class="display-label">UserId</div>
    <div class="display-field">@Model.UserId</div>

    <div class="display-label">EventId</div>
    <div class="display-field">@Model.EventId</div>

    <div class="display-label">PlaceId</div>
    <div class="display-field">@Model.PlaceId</div>

    <div class="display-label">BeepIdFather</div>
    <div class="display-field">@Model.BeepIdFather</div>

    <div class="display-label">DateInsert</div>
    <div class="display-field">@String.Format("{0:g}", Model.DateInsert)</div>

    <div class="display-label">DateUpdate</div>
    <div class="display-field">@String.Format("{0:g}", Model.DateUpdate)</div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.BeepId }) |
    @Html.ActionLink("Back to List", "Index")
</p>

